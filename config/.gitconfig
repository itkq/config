[user]
  name =
  email = kuogsi@gmail.com
[include]
  path = .gitconfig.local
[color]
  ui = true
[core]
  excludesfile = ~/.gitignore
  editor = vim
  filemode = true
[alias]
  co = checkout
  st = status -sb
  pr = pull --rebase
  fo = fetch origin
  ro = rebase origin
  rc = rebase --continue
  wd = diff --word-diff
  gp = grep -n
  lg = log --graph --pretty=oneline --decorate --date=short --abbrev-commit --branches
[init]
  templatedir = ~/.git-template
[push]
	default = simple
[branch]
	autosetuprebase = always
[pull]
  rebase = true
[commit]
	template = ~/.commit_template
